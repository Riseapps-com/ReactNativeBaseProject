{
  "name": "ReactNativeBaseProject",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "clear-cache": "watchman watch-del-all && rm -rf node_modules/ && npm cache verify && yarn install && npm start -- --reset-cache",
    "start": "react-native start",
    "compile": "tsc",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --fix && prettier -c '**/*.{json,yml,md}' --write",
    "test": "jest --maxWorkers=50%",
    "test:watch": "jest --watch --maxWorkers=25%",
    "test:coverage": "jest --collect-coverage",
    "madge:cyclic": "madge -c ./",
    "verify": "yarn compile && yarn lint && yarn test --passWithNoTests && yarn madge:cyclic",
    "prestorybook": "rnstl",
    "storybook": "start-storybook -p 7007",
    "build-storybook": "build-storybook",
    "e2e": "ts-node ./scripts/e2e-ios.ts && ts-node ./scripts/e2e-android.ts",
    "e2e:withSimulator": "ts-node ./scripts/e2e-ios.ts --withSimulator && ts-node ./scripts/e2e-android.ts",
    "e2e:ios": "ts-node ./scripts/e2e-ios.ts",
    "e2e:ios:withSimulator": "ts-node ./scripts/e2e-ios.ts --withSimulator",
    "e2e:android": "ts-node ./scripts/e2e-android.ts",
    "detox:clean": "detox clean-framework-cache && detox build-framework-cache"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "~1.15.6",
    "@react-native-community/masked-view": "~0.1.11",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/stack": "^6.0.7",
    "axios": "^0.21.1",
    "color": "^4.0.1",
    "i18next": "^20.4.0",
    "mobx": "^6.3.2",
    "mobx-react-lite": "^3.2.0",
    "react": "17.0.2",
    "react-i18next": "^11.11.4",
    "react-native": "0.65.1",
    "react-native-bootsplash": "~3.2.4",
    "react-native-fast-image": "~8.3.7",
    "react-native-gesture-handler": "~1.10.3",
    "react-native-get-random-values": "~1.7.0",
    "react-native-localize": "~2.1.1",
    "react-native-reanimated": "~1.13.3",
    "react-native-safe-area-context": "~3.3.0",
    "react-native-screens": "~3.5.0",
    "react-native-snackbar": "~2.4.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/runtime": "^7.15.3",
    "@storybook/react-native": "^5.3.25",
    "@storybook/react-native-server": "^5.3.23",
    "@testing-library/jest-native": "^4.0.2",
    "@testing-library/react-hooks": "^7.0.1",
    "@testing-library/react-native": "^7.2.0",
    "@types/color": "^3.0.2",
    "@types/detox": "^17.14.2",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.7.1",
    "@types/react": "^17.0.19",
    "@types/react-native": "^0.64.13",
    "@types/shelljs": "^0.8.9",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "axios-mock-adapter": "^1.20.0",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "detox": "^18.20.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.24.1",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-jest-formatting": "^3.0.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-react-native": "^3.11.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.1",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^11.1.2",
    "madge": "^5.0.1",
    "metro-react-native-babel-preset": "^0.66.2",
    "prettier": "^2.3.2",
    "prettier-eslint": "^13.0.0",
    "react-dom": "17.0.2",
    "react-native-codegen": "^0.0.7",
    "react-native-storybook-loader": "^2.0.4",
    "react-native-typescript-transformer": "^1.2.13",
    "react-test-renderer": "17.0.2",
    "shelljs": "^0.8.4",
    "snapshot-diff": "^0.9.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.3.5",
    "weak-napi": "^2.0.2"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src/modules"
      ],
      "pattern": "**/*.stories.tsx",
      "outputFile": "./src/modules/storybook/generated/storyLoader.js"
    }
  }
}
