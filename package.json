{
  "name": "ReactNativeBaseProject",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "clear-cache": "watchman watch-del-all && rm -rf node_modules/ && npm cache verify && yarn install && npm start -- --reset-cache",
    "start": "react-native start",
    "compile": "tsc",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --fix && prettier -c '**/*.{json,yml,md}' --write",
    "test": "jest --maxWorkers=50%",
    "test:watch": "jest --watch --maxWorkers=25%",
    "test:coverage": "jest --collect-coverage",
    "madge:cyclic": "madge -c ./",
    "verify": "yarn compile && yarn lint && yarn test --passWithNoTests && yarn madge:cyclic",
    "e2e": "ts-node ./scripts/e2e-ios.ts && ts-node ./scripts/e2e-android.ts",
    "e2e:withSimulator": "ts-node ./scripts/e2e-ios.ts --withSimulator && ts-node ./scripts/e2e-android.ts",
    "e2e:ios": "ts-node ./scripts/e2e-ios.ts",
    "e2e:ios:withSimulator": "ts-node ./scripts/e2e-ios.ts --withSimulator",
    "e2e:android": "ts-node ./scripts/e2e-android.ts",
    "detox:clean": "detox clean-framework-cache && detox build-framework-cache"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "~1.15.9",
    "@react-native-community/masked-view": "~0.1.11",
    "@react-navigation/native": "~6.0.5",
    "@react-navigation/stack": "~6.0.10",
    "axios": "^0.22.0",
    "color": "^4.0.1",
    "i18next": "^21.2.4",
    "mobx": "^6.3.3",
    "mobx-react-lite": "^3.2.1",
    "react": "17.0.2",
    "react-i18next": "^11.12.0",
    "react-native": "0.66.0",
    "react-native-bootsplash": "~3.2.6",
    "react-native-fast-image": "~8.5.11",
    "react-native-gesture-handler": "~1.10.3",
    "react-native-get-random-values": "~1.7.0",
    "react-native-localize": "~2.1.5",
    "react-native-reanimated": "1.13.3",
    "react-native-safe-area-context": "~3.3.2",
    "react-native-screens": "~3.8.0",
    "react-native-snackbar": "~2.4.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/runtime": "^7.15.4",
    "@testing-library/jest-native": "^4.0.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/react-native": "^7.2.0",
    "@types/color": "^3.0.2",
    "@types/detox": "^17.14.2",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.3",
    "@types/react": "^17.0.27",
    "@types/react-native": "^0.65.5",
    "@types/shelljs": "^0.8.9",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "axios-mock-adapter": "^1.20.0",
    "babel-jest": "^27.2.5",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "detox": "18.22.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^25.0.1",
    "eslint-plugin-jest-formatting": "^3.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-react-native": "^3.11.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.2",
    "jest": "^27.2.5",
    "jest-circus": "^27.2.5",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^11.2.2",
    "madge": "^5.0.1",
    "metro-react-native-babel-preset": "^0.66.2",
    "prettier": "^2.4.1",
    "prettier-eslint": "^13.0.0",
    "react-dom": "17.0.2",
    "react-native-typescript-transformer": "^1.2.13",
    "react-test-renderer": "17.0.2",
    "shelljs": "^0.8.4",
    "snapshot-diff": "^0.9.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  }
}
