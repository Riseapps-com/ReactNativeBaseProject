{
  "name": "ReactNativeBaseProject",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "npx jetify",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "clear-cache": "watchman watch-del-all && rm -rf node_modules/ && npm cache verify && yarn install && npm start -- --reset-cache",
    "start": "react-native start",
    "compile": "tsc",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --fix && prettier -c '**/*.{ts,tsx,js,jsx,json,yml,md}' --write",
    "test": "jest",
    "test:coverage": "jest --collect-coverage",
    "madge:cyclic": "madge -c ./",
    "verify": "yarn compile && yarn lint && yarn test --passWithNoTests && yarn madge:cyclic",
    "prestorybook": "rnstl",
    "storybook": "start-storybook -p 7007",
    "build-storybook": "build-storybook",
    "e2e": "ts-node ./scripts/e2e-ios.ts && ts-node ./scripts/e2e-android.ts",
    "e2e:withSimulator": "ts-node ./scripts/e2e-ios.ts --withSimulator && ts-node ./scripts/e2e-android.ts",
    "e2e:ios": "ts-node ./scripts/e2e-ios.ts",
    "e2e:ios:withSimulator": "ts-node ./scripts/e2e-ios.ts --withSimulator",
    "e2e:android": "ts-node ./scripts/e2e-android.ts",
    "detox:clean": "detox clean-framework-cache && detox build-framework-cache",
    "prepare": "husky install"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "~1.15.5",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/native": "^5.9.4",
    "@react-navigation/stack": "^5.14.5",
    "axios": "^0.21.1",
    "color": "^3.1.3",
    "i18next": "^20.3.1",
    "mobx": "^6.3.2",
    "mobx-react-lite": "^3.2.0",
    "react": "17.0.1",
    "react-i18next": "^11.10.0",
    "react-native": "0.64.2",
    "react-native-bootsplash": "^3.2.3",
    "react-native-fast-image": "~8.3.4",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-get-random-values": "~1.7.0",
    "react-native-localize": "~2.1.1",
    "react-native-reanimated": "~2.2.0",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "~3.3.0",
    "react-native-snackbar": "~2.4.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.14.0",
    "@storybook/react-native": "^5.3.25",
    "@storybook/react-native-server": "^5.3.23",
    "@testing-library/jest-native": "^4.0.1",
    "@testing-library/react-hooks": "^5.1.3",
    "@testing-library/react-native": "^7.2.0",
    "@types/color": "^3.0.1",
    "@types/detox": "^17.14.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.1",
    "@types/react": "^17.0.9",
    "@types/react-native": "^0.64.10",
    "@types/shelljs": "^0.8.8",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "axios-mock-adapter": "^1.19.0",
    "babel-jest": "^27.0.2",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "detox": "^18.16.0",
    "eslint": "^7.28.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jest-formatting": "^3.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-react-native": "^3.11.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^11.0.0",
    "madge": "^4.0.2",
    "metro-react-native-babel-preset": "^0.66.0",
    "prettier": "^2.3.1",
    "prettier-eslint": "^12.0.0",
    "react-dom": "^17.0.2",
    "react-native-storybook-loader": "^2.0.4",
    "react-native-typescript-transformer": "^1.2.13",
    "react-test-renderer": "^17.0.1",
    "shelljs": "^0.8.4",
    "snapshot-diff": "^0.8.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.3.2"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src/modules"
      ],
      "pattern": "**/*.stories.tsx",
      "outputFile": "./src/modules/storybook/generated/storyLoader.js"
    }
  }
}
